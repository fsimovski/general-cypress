{
    "scripts": {
        "cypress:open": "node_modules/.bin/cypress open",
        "cypress:specific": "cypress run --spec 'cypress/integration/tests/*.js'  --reporter cypress-mochawesome-reporter",
        "cypress:chrome": "cypress run --browser chrome --spec 'cypress/integration/tests/*.js' --reporter cypress-mochawesome-reporter",
        "cypress:firefox": "cypress run --browser firefox --spec 'cypress/integration/tests/*.js' --reporter cypress-mochawesome-reporter",
        "cypress:edge": "cypress run --browser edge --spec 'cypress/integration/tests/*.js' --reporter cypress-mochawesome-reporter",
        "cypress": "cypress run --record --key 064f11fa-8e8a-433e-a774-4d6002f18545 --spec 'cypress/integration/tests/*.js'"
    },
    "devDependencies": {
        "cypress": "^5.4.0",
        "cypress-mochawesome-reporter": "^1.0.3",
        "cypress-multi-reporters": "^1.4.0",
        "mocha-junit-reporter": "^2.0.0",
        "mochawesome": "^6.1.1",
        "mochawesome-merge": "^4.2.0",
        "mochawesome-report-generator": "^5.1.0"
    }
}